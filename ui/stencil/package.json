{
  "name": "@bestsolution/quti",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/quti/quti.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BestSolution-at/quti.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "npm run lint:eslint && npm run lint:css && npm run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:css": "stylelint '**/*.{css,html}'",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.1",
    "@stencil/core": "^4.7.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-gitlab": "^5.1.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.2.5",
    "puppeteer": "^21.9.0",
    "stylelint": "^16.2.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-formatter-gitlab": "^1.0.2"
  },
  "license": "GPL-3.0"
}
